package arr_p;

import org.apache.catalina.tribes.util.Arrays;

public class SortMain {

	public static void main(String[] args) {
		// 77, 65, 98, 46, 82
		
		// 46, 65, 77, 82, 98
		
		int a = 10;
		int b = 20;
		
		System.out.println(a+","+b);
		
		int buf = b;
		b = a;
		a = buf;
		
		System.out.println(a+","+b);
		
		int [] arr = {77, 65, 98, 46, 82};
		System.out.println("정렬전: " +Arrays.toString(arr));

	}

}
