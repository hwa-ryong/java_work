package basic_p;

public class TSNMain {

	public static void main(String[] args) {
		
		System.out.println("3,6,9 게임");
		System.out.println("1 -> 20");
		
		for (int i = 1; i<=20; i++) {
			int one = i % 10;
			if(one % 3 == 0 && one != 0) {
				System.out.println("짝");
			}else {
				System.out.println(i+","+one);
			}
			
		}
		
		
		int no = 1256;
		for (int i = 1; i<=no; i++) {
			
			String ttt = "";
			
			int buf = i;
		}
	}

}
