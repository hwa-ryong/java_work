package basic_p;

public class TSNMain {

	public static void main(String[] args) {
		
		System.out.println("3,6,9 게임");
		System.out.println("1 -> 20");
		
		int one = i % 10;
		
		try {
			
			for(int i=1; i<=20; i++) {
				if(one % 3 == 0 && one != 0) {
					System.out.println("짝");
				}else {
					System.out.println(i);
				}
			}
			
		}catch (Exception e){
			System.out.println("예외 처리 되었습니다.");
		}
	
	}

}
